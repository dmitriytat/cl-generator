{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-async-functions",
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals"
  ]
}
